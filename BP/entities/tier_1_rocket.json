{
  "format_version": "1.16.220",
  "minecraft:entity": {
    "description": {
      "identifier": "cosmo:tier_1_rocket",
      "is_spawnable": true,
      "is_summonable": true,
      "animations": {
        "launch": "animation.tier_1_launch"
      },
      "scripts": {
        "animate": [
          {
            "launch": "query.has_rider"
          }
        ]
      }
    },
    "component_groups": {},
    "components": {
      "minecraft:physics": {
        "has_collision": true,
        "has_gravity": true
      },
      "minecraft:collision_box": {
        "height": 2,
        "width": 1
      },
      "minecraft:damage_sensor": {},
      "minecraft:rideable": {
        "controlling_seat": 1,
        "interact_text": "action.interact.tier_1_launch",
        "seats": [
          {
            "position": [
              0,
              0.4,
              0
            ]
          }
        ]
      }
    },
    "events": {
      "launch": {
        "run_command": {
          "command": [
            "execute @p ~ ~ ~ function moon/launch",
            "kill @s"
          ]
        }
      },
      "launch_end": {
        "run_command": {
          "command": [
            "execute @p ~ ~ ~ function overworld/return",
            "kill @s"
          ]
        }
      },
      "launch_multi": {
        "sequence": [
          {
            "trigger": "launch",
            "filters": {
              "test": "has_biome_tag",
              "operator": "!=",
              "value": "the_end"
            }
          },
          {
            "trigger": "launch_end",
            "filters": {
              "test": "has_biome_tag",
              "operator": "==",
              "value": "the_end"
            }
          }
        ]
      }
    }
  }
}